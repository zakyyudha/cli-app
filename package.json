{
  "name": "@zakyyudha/cli-app",
  "version": "0.1.0",
  "description": "A CLI package that wraps commander.js and mongo migration",
  "main": "dist/index.js",
  "bin": {
    "cli-app": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && mkdir -p dist/stubs && cp -v src/stubs/* dist/stubs/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy:local": "rm -rf dist && npm run build && npm link",
    "release": "release-it"
  },
  "dependencies": {
    "cli-table": "^0.3.11",
    "commander": "^13.1.0",
    "migrate-mongo": "^12.1.3"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.4",
    "@types/migrate-mongo": "^10.0.6",
    "@types/node": "^20.19.31",
    "eslint": "^8.57.1",
    "eslint-config-love": "^48.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "mongodb": "^7.1.0",
    "prettier": "^3.8.1",
    "release-it": "^19.2.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "author": "Zaky Yudha",
  "repository": {
    "type": "git",
    "url": "https://github.com/zakyyudha/cli-app.git"
  },
  "bugs": {
    "url": "https://github.com/zakyyudha/cli-app/issues"
  },
  "homepage": "https://github.com/zakyyudha/cli-app#readme",
  "keywords": [
    "cli",
    "typescript",
    "mongo",
    "postgresql",
    "kafka",
    "redis"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.22.0"
  }
}
